//CS Text Review #2 
#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int num;
    bool tnt; 
    srand(ctime(0));
    tnt = true;
    for(int i; i<21; i++) { 
            num = 17 + rand()%13; 
            cout<<num<<endl;
            }
    if (num%6==0||num%4==0) 
    cout<<"Multiple of 6 or 4"<<endl;
    tnt = false;   
    if (num%3==0||num%5==0)
    cout<<"Multiple of 3 or 5"<<endl;
    else cout<<"Not a multiple of 3, 4, 5, or 6"<<endl;
    
    system("pause");
    return 0;
}
    
